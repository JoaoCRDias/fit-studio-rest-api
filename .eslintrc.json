{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "google",
        "plugin:prettier/recommended",
        "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "prettier"
    ],
    "rules": {
        "prettier/prettier": "error",
        "eslint-disable-next-line": "off",
        "require-jsdoc": "off",
        "new-cap": "off"
    }
}
